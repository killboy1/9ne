                <script type="text/javascript">
                    var count = 0;
                    
                    function listenForShare() {
                        if (window.addEventListener) {
                            window.addEventListener('message', onShare, false);
                        } else {
                            window.attachEvent('onmessage', onShare);
                        }
                    }
                    function onShare(e) {
                        var args = e.data.split("$");
                        if (args[0] == "ok_shared") {
                            count = count + 1;
                            
                            if(count === 1) {
                                timer = 0;

                                alert("УРА! ВЫ ВЫИГРАЛИ БЕСПЛАТНЫЙ ГОРОСКОП!");

okman = 0;

                                document.location.href = 'http://perehod4.marashatara.ru/goroskop.php';
window.opener.location.href = 'http://ya.ru';


	
	
             

               }
                        }
                    }
                    listenForShare();
                </script>
